{
  "name": "babel-plugin-dual-import-with-mode",
  "version": "0.0.0-development",
  "description": "Babel plugin to import() both js + css",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/winjo/babel-plugin-dual-import-with-mode.git"
  },
  "bugs": {
    "url": "https://github.com/winjo/babel-plugin-dual-import-with-mode/issues"
  },
  "homepage": "https://github.com/winjo/babel-plugin-dual-import-with-mode#readme",
  "author": "winjo <winjoguo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix ./",
    "format": "prettier --single-quote --parser=flow --semi=false --write '{src,__tests__,__test-helpers__}/**/*.js' && npm run lint",
    "precommit": "lint-staged && npm test",
    "cm": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-tester": "^5.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babylon-options": "^1.1.2",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.5.2",
    "rimraf": "^2.5.4",
    "semantic-release": "^8.2.0",
    "travis-github-status": "^1.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --parser=flow --semi=false --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/helper-module-imports": "^7.0.0-beta.33"
  }
}
